{
    "name": "gatsby-gravityforms-component",
    "version": "2.0.0",
    "description": "A component to take gatsby-source-gravityforms query data and return usable components",
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src --out-dir ./dist --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir ./dist --ignore **/__tests__",
        "pretest": "eslint --ignore-path .gitignore . --fix",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robmarshall/gatsby-gravityforms-component.git"
    },
    "keywords": [
        "gravity",
        "forms",
        "gravityforms",
        "WordPress",
        "form",
        "Gatsby"
    ],
    "author": "Robert Marshall",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/robmarshall/gatsby-gravityforms-component/issues"
    },
    "homepage": "https://github.com/robmarshall/gatsby-gravityforms-component#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "classnames": "^2.2.6",
        "dotenv": "^8.0.0",
        "lodash": "^4.17.15",
        "nanoid": "^3.1.3",
        "oauth-signature": "^1.5.0",
        "prop-types": "^15.7.2",
        "react-hook-form": "^5.6.0",
        "react-html-parser": "^2.0.2",
        "reaptcha": "^1.7.2",
        "sanitize-html": "^1.23.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.9.0",
        "babel-eslint": "^10.1.0",
        "babel-preset-gatsby-package": "^0.3.2",
        "cross-env": "^7.0.2",
        "eslint": "^6.5.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-import-order-alphabetical": "^1.0.0",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-sort-destructure-keys": "^1.3.4",
        "gatsby-plugin-eslint": "^2.0.5",
        "install-peers": "^1.0.3"
    },
    "peerDependencies": {
        "gatsby": "^2.18.25",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    }
}

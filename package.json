        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-order-alphabetical": "^1.0.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-sort-destructure-keys": "^1.3.5",
        "gatsby-plugin-eslint": "^2.0.5",
        "install-peers": "^1.0.3"
